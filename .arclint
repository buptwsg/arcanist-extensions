{
    "linters": {
        "json": {
            "type": "json",
            "include": [
                "(^\\.arcconfig$)",
                "(^\\.arclint$)",
                "(configs/javascript/eslint.*)"
            ]
        },
        "generated": {
            "type": "generated"
        },
        "chmod": {
            "type": "chmod"
        },
        "merge-conflict": {
            "type": "merge-conflict"
        },
        "spelling": {
            "type": "spelling"
        },
        "text": {
            "type": "text",
            "text.max-line-length": 120
        },
        "php": {
            "type": "phutil-library",
            "include": "(\\.php$)"
        }
    }
}
