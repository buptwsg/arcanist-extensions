{
    "linters": {
        "json": {
            "type": "json",
            "include": [
                "(^\\.arcconfig$)",
                "(^\\.arclint$)",
                "(configs/javascript/eslint.*)"
            ]
        },
        "chmod": {
            "type": "chmod"
        },
        "merge-conflict": {
            "type": "merge-conflict"
        },
        "spelling": {
            "type": "spelling"
        },
        "text": {
            "type": "text"
        },
        "php": {
            "type": "phutil-library",
            "include": "(\\.php$)"
        }
    }
}
